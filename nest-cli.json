{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/agrisistance-land-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/agrisistance-land-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/agrisistance-land-service",
  "projects": {
    "agrisistance-land-service": {
      "type": "application",
      "root": "apps/agrisistance-land-service",
      "entryFile": "main",
      "sourceRoot": "apps/agrisistance-land-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agrisistance-land-service/tsconfig.app.json"
      }
    },
    "agrisistance-api-gateway": {
      "type": "application",
      "root": "apps/agrisistance-api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/agrisistance-api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agrisistance-api-gateway/tsconfig.app.json"
      }
    },
    "agrisistance-user-service": {
      "type": "application",
      "root": "apps/agrisistance-user-service",
      "entryFile": "main",
      "sourceRoot": "apps/agrisistance-user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agrisistance-user-service/tsconfig.app.json"
      }
    },
    "agrisistance-network-service": {
      "type": "application",
      "root": "apps/agrisistance-network-service",
      "entryFile": "main",
      "sourceRoot": "apps/agrisistance-network-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agrisistance-network-service/tsconfig.app.json"
      }
    }
  }
}